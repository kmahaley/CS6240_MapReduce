INPUT_PATH = <PROVIDE INPUT TO INPUT FILES ON CLUSTER>
INTER_OUTPUT = <PROVIDE PATH TO A FOLDER TO CREATE A INTERMEDIATE OUTPUT>
OUTPUT_PATH =  <PROVIDE PATH TO OUTPUT FOLDER ON CLUSTER>
LOCAL_FOLDER= <PROVIDE PATH ON LOCAL SYSTEM WHERE YOU WANT TO DOWNLOAD ALL FILES>
paths:
	java -cp uber-perf-0.0.1.jar neu.perf.App -kind=cloud -arguments="$(INPUT_PATH) $(INTER_OUTPUT) $(OUTPUT_PATH) -h" -name=a5_run
	aws s3 sync $(OUTPUT_PATH)_a5_run_Step_0 $(LOCAL_FOLDER)
	cat $(LOCAL_FOLDER)/part-* > $(LOCAL_FOLDER)/hadoop-output
